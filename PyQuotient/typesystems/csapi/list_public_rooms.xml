<typesystem package="PyQuotient">
  <load-typesystem name="typesystem_core.xml" generate="no" />
  
  <load-typesystem name="../jobs/typesystem_basejob.xml" generate="no" />

  <namespace-type name="Quotient">
    <object-type name="GetRoomVisibilityOnDirectoryJob" />
    <object-type name="SetRoomVisibilityOnDirectoryJob" />
    <object-type name="GetPublicRoomsJob" />
    <object-type name="QueryPublicRoomsJob">
      <!-- Inner data structures -->
      <value-type name="Filter" />
    </object-type>
  </namespace-type>
</typesystem>
